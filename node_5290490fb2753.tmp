const readline = require('readline');
 
const rl = readline.createInterface({
  input: process.stdin,
  output: process.stdout
});
 
rl.question('Qual é o seu nome? ', (answer) => {
  // TODO: Log the answer in a database
  console.log(`Obrigado, $`);
 
  rl.close();
});

